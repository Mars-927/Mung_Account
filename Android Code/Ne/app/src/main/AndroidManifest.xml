<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mungbill">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Bill.UniteApp"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MungBill"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Analyse.AnalyseActivity"
            android:exported="true"
            android:theme="@style/Theme.MyApplication.ActionBar"/>
        <activity
            android:name=".Bill.record_Activity"
            android:exported="true"
            android:theme="@style/Theme.MyApplication.ActionBar"></activity>
        <activity
            android:name=".Bill.BillMain"
            android:exported="true"
            android:theme="@style/Theme.MyApplication.ActionBar">
        </activity>
        <activity
            android:name=".Init.Initial"
            android:exported="true"
            android:theme="@style/LunchTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginSignin.Signin"
            android:exported="true"
            android:theme="@style/Theme.MyApplication.ActionBar" />
        <activity
            android:name=".LoginSignin.LoginSignin"
            android:exported="true"
            android:theme="@style/Theme.MyApplication.ActionBar" />
    </application>

</manifest>